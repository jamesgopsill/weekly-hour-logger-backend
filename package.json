    "@types/jest": "^28.1.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "jest": "^28.1.2",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "supertest": "^6.2.3",
